{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "module": "NodeNext",
    "target": "ESNext",
    "isolatedModules": true,
    "noEmit": true,
    "removeComments": true,
    "strict": true,
    "noImplicitOverride": true,
    // "noPropertyAccessFromIndexSignature": true,
    // "noUncheckedIndexedAccess": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "allowUnusedLabels": false,
    "noUnusedParameters": true
  },
  "exclude": [
    "./coverage" // temp
  ]
}